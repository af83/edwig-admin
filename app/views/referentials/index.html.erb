<div class="container">
  <div class="row">
    <h1 class="text-center">Choisissez un Referential</h1>
    <div class="col-md-12">
      <div class="panel panel-default panel-table">
        <div class="panel-heading">
          <div class="row">
            <div class="col col-xs-6">
              <button type="button" class="btn btn-sm btn-primary btn-create">Create New Referential</button>
            </div>
          </div>
        </div>
        <div class="panel-body">
          <table class="table table-striped table-bordered table-list">
            <thead>
            <tr>
              <th class="hidden-xs">ID</th>
              <th>Slug</th>
              <th>NextReloadAt</th>
              <th>Token</th>
              <th><em class="fa fa-cog"></em></th>
            </tr>
            </thead>
            <tbody>
            <!-- TR d'exemple -->
            <% @referentialsTab.each do |i| %>
                <tr>
                  <td class="hidden-xs"><%= i["Id"] %></td>
                  <td><%= i["Slug"] %></td>
                  <td><%= i["NextReloadAt"] %></td>
                  <td><%= i["Tokens"][0] %></td>
                  <td align="center">
                    <%= link_to (raw("<em class=\"fa fa fa-eye\"></em>")), partners_path, class: "btn btn-default" %>

                    <!--<a class="btn btn-default"><em class="fa fa-eye"></em></a>-->
                    <a class="btn btn-default"><em class="fa fa-pencil"></em></a>
                    <a class="btn btn-danger"><em class="fa fa-trash"></em></a>
                  </td>
                </tr>
            <% end %>
            </tbody>
          </table>
        </div>
